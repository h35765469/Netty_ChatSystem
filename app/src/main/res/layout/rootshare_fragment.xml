<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#e60000">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/collectShareBackToProfileImg"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:src="@drawable/leftarrow" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="收藏"
            android:id="@+id/textView23"
            android:layout_centerInParent="true"
            android:textColor="#ffffff" />
    </RelativeLayout>

    <com.cool.user.netty_chatsystem.Chat_Fragment.ProfileFragment.CollectFragment.RecyclerViewPager.RecyclerViewPager
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/recycleViewPager">

    </com.cool.user.netty_chatsystem.Chat_Fragment.ProfileFragment.CollectFragment.RecyclerViewPager.RecyclerViewPager>


</LinearLayout>